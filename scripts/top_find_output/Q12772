    var dataarr = [];
    var catarr = [];


    $(document).ready(

        function makeArr() {

        var domainElem = [["Sequence","1-1141",0,1141],["Chains","1-1141",1,1141],["Chains","1-484",1,484],["N-Terms","1",1,2],["N-Terms","469",469,470],["N-Terms","485",485,486],["N-Terms","491",491,492],["N-Terms","523",523,524],["C-Terms","468",467,468],["C-Terms","484",483,484],["C-Terms","490",489,490],["C-Terms","522",521,522],["C-Terms","1068",1067,1068],["C-Terms","1141",1140,1141],["Cleavages","468",467,468],["Cleavages","468",467,468],["Cleavages","468",467,468],["Cleavages","484",483,484],["Cleavages","490",489,490],["Cleavages","522",521,522],["Topology","TURN (366-368)",366,368],["Topology","HELIX (346-357)",346,357],["Topology","HELIX (369-399)",369,399],["Domains","REGION (1-50)",1,50],["Domains","REGION (237-491)",237,491],["Domains","REGION (380-401)",380,401],["Domains","DOMAIN (330-380)",330,380],["Variants","MUTAGEN (478-481)",478,481],["Variants","MUTAGEN (478-481)",478,481],["Variants","MUTAGEN (478-478)",478,478],["Variants","MUTAGEN (479-481)",479,481],["Variants","MUTAGEN (479-479)",479,479],["Variants","MUTAGEN (481-481)",481,481],["Variants","MUTAGEN (484-485)",484,485],["Variants","MUTAGEN (484-484)",484,484],["Variants","MUTAGEN (485-485)",485,485],["Variants","MUTAGEN (490-491)",490,491],["Variants","MUTAGEN (495-496)",495,496],["Variants","MUTAGEN (495-495)",495,495],["Variants","MUTAGEN (496-496)",496,496],["Variants","MUTAGEN (519-519)",519,519],["Variants","MUTAGEN (519-519)",519,519],["Topology","TOPO_DOM (1-479)",1,479],["Topology","TOPO_DOM (501-533)",501,533],["Topology","TOPO_DOM (555-1139)",555,1139],["Topology","TRANSMEM (480-500)",480,500],["Topology","TRANSMEM (534-554)",534,554],["Variants","VARIANT (273-273)",273,273],["Variants","VARIANT (347-347)",347,347],["Variants","VARIANT (536-536)",536,536],["Variants","VARIANT (595-595)",595,595],["Variants","VARIANT (623-623)",623,623],["Variants","VARIANT (860-860)",860,860],["Variants","CONFLICT (961-961)",961,961],["Variants","CONFLICT (1045-1045)",1045,1045],["Variants","VAR_SEQ (273-275)",273,275],["Variants","VAR_SEQ (589-684)",589,684],["Variants","VAR_SEQ (685-1141)",685,1141],["Domains","SITE (468-469)",468,469],["Domains","SITE (484-485)",484,485],["Domains","SITE (522-523)",522,523],["Topology","TOPO_DOM (1-479)",1,479],["Topology","TOPO_DOM (501-533)",501,533],["Topology","TOPO_DOM (555-1139)",555,1139],["Topology","TRANSMEM (480-500)",480,500],["Topology","TRANSMEM (534-554)",534,554],["Domains","DOMAIN (330-380)",330,380],["Domains","REGION (1-50)",1,50],["Domains","REGION (237-491)",237,491],["Domains","REGION (380-401)",380,401],["Domains","SITE (468-469)",468,469],["Domains","SITE (484-485)",484,485],["Domains","SITE (522-523)",522,523],["Modifications","MOD_RES (855-855)",855,855],["Modifications","MOD_RES (1098-1098)",1098,1098],["Topology","CROSSLNK (464-464)",464,464],["Variants","VAR_SEQ (273-275)",273,275],["Variants","VAR_SEQ (589-684)",589,684],["Variants","VAR_SEQ (685-1141)",685,1141],["Variants","VARIANT (273-273)",273,273],["Variants","VARIANT (347-347)",347,347],["Variants","VARIANT (536-536)",536,536],["Variants","VARIANT (595-595)",595,595],["Variants","VARIANT (623-623)",623,623],["Variants","VARIANT (860-860)",860,860],["Variants","MUTAGEN (478-481)",478,481],["Variants","MUTAGEN (478-481)",478,481],["Variants","MUTAGEN (478-478)",478,478],["Variants","MUTAGEN (479-481)",479,481],["Variants","MUTAGEN (479-479)",479,479],["Variants","MUTAGEN (481-481)",481,481],["Variants","MUTAGEN (484-485)",484,485],["Variants","MUTAGEN (484-484)",484,484],["Variants","MUTAGEN (485-485)",485,485],["Variants","MUTAGEN (490-491)",490,491],["Variants","MUTAGEN (495-496)",495,496],["Variants","MUTAGEN (495-495)",495,495],["Variants","MUTAGEN (496-496)",496,496],["Variants","MUTAGEN (519-519)",519,519],["Variants","MUTAGEN (519-519)",519,519],["Variants","CONFLICT (961-961)",961,961],["Variants","CONFLICT (1045-1045)",1045,1045],["Topology","HELIX (346-357)",346,357],["Topology","HELIX (369-399)",369,399],["Topology","TURN (366-368)",366,368]];

        var dataarr = [];
        var catarr = [];
        var i;

        for (i = 0; i < domainElem.length ; i++ )
        {
        switch(domainElem[i][0]) {
            
          case "Sequence":
                catarr[0] = (domainElem[i][0]);
                dataarr.push({
                x:parseInt(domainElem[i][2]),
                x2: parseInt(domainElem[i][3]),
                y:0,
                color: '#e41a1c',
                key: (domainElem[i][1])
                });
                break;

            
            case "Chains":
                catarr[1] = (domainElem[i][0]);
                dataarr.push({
                x:parseInt(domainElem[i][2]),
                x2: parseInt(domainElem[i][3]),
                y:1,
                color: '#377eb8',
                key: (domainElem[i][1])
                });
                break;
            
            case "Topology":
                catarr[2] = (domainElem[i][0]);
                dataarr.push({
                x: parseInt(domainElem[i][2]),
                x2: parseInt(domainElem[i][3]),
                y:2,
                color: '#4daf4a',
                key: (domainElem[i][1])
                });
                break;
            
            case "N-Terms":
                catarr[3] = (domainElem[i][0]);
                dataarr.push({
                x:parseInt(domainElem[i][2]),
                x2: parseInt(domainElem[i][3]),
                y:3,
                color: '#984ea3',
                key: (domainElem[i][1])
                });
                break;
            
            case "C-Terms":
                catarr[4] = (domainElem[i][0]);
                dataarr.push({
                x:parseInt(domainElem[i][2]),
                x2: parseInt(domainElem[i][3]),
                y:4,
                color: '#ff7f00',
                key: (domainElem[i][1])
                });
                break;
            
            case "Cleavages":
                catarr[5] = (domainElem[i][0]);
                dataarr.push({
                x:parseInt(domainElem[i][2]),
                x2: parseInt(domainElem[i][3]),
                y:5,
                color: '#CCCC00',
                key: (domainElem[i][1])
                });
                break;
            
            case "Modifications":
                catarr[6] = (domainElem[i][0]);
                dataarr.push({
                x:parseInt(domainElem[i][2]),
                x2: parseInt(domainElem[i][3]),
                y:6,
                color: '#a65628',
                key: (domainElem[i][1])
                });
                break;
            
            case "Variants":
                catarr[7] = (domainElem[i][0]);
                dataarr.push({
                x:parseInt(domainElem[i][2]),
                x2: parseInt(domainElem[i][3]),
                y:7,
                color: '#f781bf',
                key: (domainElem[i][1])
                });
                break;
            
            case "Domains":
                catarr[8] = (domainElem[i][0]);
                dataarr.push({
                x:parseInt(domainElem[i][2]),
                x2: parseInt(domainElem[i][3]),
                y:8,
                color: '#999999',
                key: (domainElem[i][1])
                });
                break;
            
        }
        }


    

    (function (H) {
    var defaultPlotOptions = H.getOptions().plotOptions,
    columnType = H.seriesTypes.column,
    each = H.each,
    extendClass = H.extendClass,
    pick = H.pick,
    Point = H.Point,
    Series = H.Series;
    
    defaultPlotOptions.xrange = H.merge(defaultPlotOptions.column, {
    tooltip: {
    headerFormat: '<span style="font-size: 0.85em">{point.x} - {point.x2}</span><br/>',
    pointFormat: '<span style="color:{point.color}">\u25CF</span> {point.yCategory}: <b>{point.key}</b><br/>',
    footerFormat: '<span style="color:{point.color}">{series.name}</span><br/>'
    }
    });
    H.seriesTypes.xrange = H.extendClass(columnType, {
    pointClass: extendClass(Point, {
        // Add x2 and yCategory to the available properties for tooltip formats
    getLabelConfig: function () {
        var cfg = Point.prototype.getLabelConfig.call(this);
        
        cfg.x2 = this.x2;
        cfg.yCategory = this.yCategory = this.series.yAxis.categories && this.series.yAxis.categories[this.y];
        return cfg;
    }
    }),
    type: 'xrange',
    forceDL: true,
    parallelArrays: ['x', 'x2', 'y'],
    requireSorting: false,
    animate: H.seriesTypes.line.prototype.animate,
        
        /**
         * Borrow the column series metrics, but with swapped axes. This gives free access
         * to features like groupPadding, grouping, pointWidth etc.
         */
    getColumnMetrics: function () {
        var metrics,
        chart = this.chart;
        
        function swapAxes() {
            each(chart.series, function (s) {
                var xAxis = s.xAxis;
                s.xAxis = s.yAxis;
                s.yAxis = xAxis;
            });
        }
        
        swapAxes();
        
        this.yAxis.closestPointRange = 1;
        metrics = columnType.prototype.getColumnMetrics.call(this);
        
        swapAxes();
        
        return metrics;
    },
        
        /**
         * Override cropData to show a point where x is outside visible range
         * but x2 is outside.
         */
    cropData: function (xData, yData, min, max) {
        
        // Replace xData with x2Data to find the appropriate cropStart
        var crop = Series.prototype.cropData.call(this, this.x2Data, yData, min, max);
        
        // Re-insert the cropped xData
        crop.xData = xData.slice(crop.start, crop.end);
        
        return crop;
    },
        
    translate: function () {
        columnType.prototype.translate.apply(this, arguments);
        var series = this,
        xAxis = series.xAxis,
        metrics = series.columnMetrics,
        minPointLength = series.options.minPointLength || 0;
        
        H.each(series.points, function (point) {
            var plotX = point.plotX,
            plotX2 = xAxis.toPixels(H.pick(point.x2, point.x + (point.len || 0)), true),
            width = plotX2 - plotX,
            widthDifference;
            
            if (minPointLength) {
                widthDifference = width < minPointLength ? minPointLength - width : 0;
                plotX -= widthDifference / 2;
                plotX2 += widthDifference / 2;
            }
            
            plotX = Math.max(plotX, -10);
            plotX2 = Math.min(Math.max(plotX2, -10), xAxis.len + 10);
            
            point.shapeArgs = {
            x: plotX,
            y: point.plotY + metrics.offset,
            width: plotX2 - plotX,
            height: metrics.width
            };
            point.tooltipPos[0] += width / 2;
            point.tooltipPos[1] -= metrics.width / 2;
        });
    }
    });
    
    /**
     * Max x2 should be considered in xAxis extremes
     */
    H.wrap(H.Axis.prototype, 'getSeriesExtremes', function (proceed) {
        var axis = this,
        dataMax,
        modMax;
        
        proceed.call(this);
        if (this.isXAxis) {
            dataMax = pick(axis.dataMax, Number.MIN_VALUE);
            each(this.series, function (series) {
                each(series.x2Data || [], function (val) {
                    if (val > dataMax) {
                        dataMax = val;
                        modMax = true;
                    }
                });
            });
            if (modMax) {
                axis.dataMax = dataMax;
            }
        }
    });
}(Highcharts));

Highcharts.chart('container_chart', {
    chart: {
    type: 'xrange'
    },
    title: {
    text: 'Summary'
    },
    xAxis: {
    type: 'linear'
    },
    yAxis: {
    title: {
    text: ''
    },
    categories: [catarr[0], catarr[1], catarr[2], catarr[3], catarr[4], catarr[5], catarr[6], catarr[7], catarr[8]],
    reversed: true
    },
    series: [{
    name: 'Q12772',
    pointWidth: 20,
    colorByPoint: true,
    data: dataarr,
    point: {
    events: {
    }
    }
        
    }]
    });


    });

</script>


