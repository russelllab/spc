gy","TRANSMEM (325-345)",325,345],["Topology","TRANSMEM (370-390)",370,390],["Topology","TRANSMEM (396-416)",396,416],["Topology","TRANSMEM (431-451)",431,451],["Topology","TRANSMEM (458-478)",458,478],["Modifications","MOD_RES (31-31)",31,31],["Modifications","MOD_RES (35-35)",35,35],["Modifications","MOD_RES (45-45)",45,45],["Topology","CROSSLNK (19-19)",19,19],["Topology","CROSSLNK (30-30)",30,30],["Variants","VARIANT (223-223)",223,223],["Variants","VARIANT (230-230)",230,230],["Variants","MUTAGEN (117-117)",117,117],["Variants","MUTAGEN (246-246)",246,246],["Variants","MUTAGEN (252-252)",252,252],["Variants","MUTAGEN (257-257)",257,257],["Variants","MUTAGEN (258-258)",258,258],["Variants","MUTAGEN (258-258)",258,258],["Variants","MUTAGEN (259-259)",259,259],["Variants","MUTAGEN (303-303)",303,303],["Variants","MUTAGEN (375-375)",375,375],["Variants","MUTAGEN (483-507)",483,507],["Variants","CONFLICT (15-15)",15,15],["Variants","CONFLICT (29-31)",29,31],["Variants","CONFLICT (35-35)",35,35],["Variants","CONFLICT (62-62)",62,62],["Variants","CONFLICT (88-88)",88,88],["Variants","CONFLICT (154-154)",154,154],["Topology","HELIX (54-63)",54,63],["Topology","HELIX (66-69)",66,69],["Topology","HELIX (72-79)",72,79],["Topology","HELIX (82-109)",82,109],["Topology","HELIX (116-123)",116,123],["Topology","HELIX (127-136)",127,136],["Topology","HELIX (140-154)",140,154],["Topology","HELIX (168-185)",168,185],["Topology","HELIX (190-220)",190,220],["Topology","HELIX (241-253)",241,253],["Topology","HELIX (262-266)",262,266],["Topology","HELIX (274-299)",274,299],["Topology","HELIX (302-307)",302,307],["Topology","HELIX (311-319)",311,319],["Topology","HELIX (327-337)",327,337],["Topology","HELIX (341-344)",341,344],["Topology","HELIX (347-355)",347,355],["Topology","HELIX (374-385)",374,385],["Topology","HELIX (386-389)",386,389],["Topology","HELIX (393-420)",393,420],["Topology","HELIX (434-452)",434,452],["Topology","HELIX (457-461)",457,461],["Topology","HELIX (470-473)