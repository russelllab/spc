<script type="text/javascript">

    var dataarr = [];
    var catarr = [];


    $(document).ready(

        function makeArr() {

        var domainElem = [["Sequence","1-262",0,262],["Chains","1-262",1,262],["N-Terms","1",1,2],["N-Terms","69",69,70],["N-Terms","69",69,70],["N-Terms","81",81,82],["N-Terms","106",106,107],["N-Terms","106",106,107],["N-Terms","174",174,175],["N-Terms","174",174,175],["N-Terms","175",175,176],["N-Terms","175",175,176],["N-Terms","178",178,179],["C-Terms","262",261,262],["Variants","VAR_SEQ (1-1)",1,1],["Variants","VAR_SEQ (83-83)",83,83],["Variants","VAR_SEQ (114-142)",114,142],["Topology","TOPO_DOM (1-4)",1,4],["Topology","TOPO_DOM (28-262)",28,262],["Topology","TRANSMEM (5-27)",5,27],["Modifications","MOD_RES (39-39)",39,39],["Modifications","MOD_RES (40-40)",40,40],["Modifications","MOD_RES (41-41)",41,41],["Modifications","MOD_RES (43-43)",43,43],["Modifications","MOD_RES (84-84)",84,84],["Modifications","MOD_RES (101-101)",101,101],["Modifications","MOD_RES (106-106)",106,106],["Modifications","MOD_RES (110-110)",110,110],["Modifications","MOD_RES (156-156)",156,156],["Modifications","MOD_RES (161-161)",161,161],["Modifications","MOD_RES (200-200)",200,200],["Modifications","MOD_RES (220-220)",220,220],["Modifications","MOD_RES (224-224)",224,224],["Modifications","MOD_RES (240-240)",240,240],["Modifications","MOD_RES (241-241)",241,241],["Modifications","MOD_RES (255-255)",255,255],["Domains","REGION (161-164)",161,164],["Domains","REGION (200-203)",200,203],["Domains","REGION (220-223)",220,223],["Modifications","LIPID (26-26)",26,26],["Modifications","LIPID (29-29)",29,29],["Variants","MUTAGEN (26-26)",26,26],["Variants","MUTAGEN (29-29)",29,29],["Variants","MUTAGEN (161-161)",161,161],["Variants","MUTAGEN (200-200)",200,200],["Variants","MUTAGEN (220-220)",220,220],["Topology","TOPO_DOM (1-4)",1,4],["Topology","TOPO_DOM (28-262)",28,262],["Topology","TRANSMEM (5-27)",5,27],["Domains","REGION (161-164)",161,164],["Domains","REGION (200-203)",200,203],["Domains","REGION (220-223)",220,223],["Modifications","MOD_RES (39-39)",39,39],["Modifications","MOD_RES (40-40)",40,40],["Modifications","MOD_RES (41-41)",41,41],["Modifications","MOD_RES (43-43)",43,43],["Modifications","MOD_RES (84-84)",84,84],["Modifications","MOD_RES (101-101)",101,101],["Modifications","MOD_RES (106-106)",106,106],["Modifications","MOD_RES (109-109)",109,109],["Modifications","MOD_RES (110-110)",110,110],["Modifications","MOD_RES (156-156)",156,156],["Modifications","MOD_RES (161-161)",161,161],["Modifications","MOD_RES (200-200)",200,200],["Modifications","MOD_RES (220-220)",220,220],["Modifications","MOD_RES (224-224)",224,224],["Modifications","MOD_RES (240-240)",240,240],["Modifications","MOD_RES (241-241)",241,241],["Modifications","MOD_RES (255-255)",255,255],["Modifications","LIPID (26-26)",26,26],["Modifications","LIPID (29-29)",29,29],["Variants","VAR_SEQ (1-1)",1,1],["Variants","VAR_SEQ (83-83)",83,83],["Variants","VAR_SEQ (114-142)",114,142],["Variants","MUTAGEN (26-26)",26,26],["Variants","MUTAGEN (29-29)",29,29],["Variants","MUTAGEN (161-161)",161,161],["Variants","MUTAGEN (200-200)",200,200],["Variants","MUTAGEN (220-220)",220,220]];

        var dataarr = [];
        var catarr = [];
        var i;

        for (i = 0; i < domainElem.length ; i++ )
        {
        switch(domainElem[i][0]) {
            
          case "Sequence":
                catarr[0] = (domainElem[i][0]);
                dataarr.push({
                x:parseInt(domainElem[i][2]),
                x2: parseInt(domainElem[i][3]),
                y:0,
                color: '#e41a1c',
                key: (domainElem[i][1])
                });
                break;

            
            case "Chains":
                catarr[1] = (domainElem[i][0]);
                dataarr.push({
                x:parseInt(domainElem[i][2]),
                x2: parseInt(domainElem[i][3]),
                y:1,
                color: '#377eb8',
                key: (domainElem[i][1])
                });
                break;
            
            case "Topology":
                catarr[2] = (domainElem[i][0]);
                dataarr.push({
                x: parseInt(domainElem[i][2]),
                x2: parseInt(domainElem[i][3]),
                y:2,
                color: '#4daf4a',
                key: (domainElem[i][1])
                });
                break;
            
            case "N-Terms":
                catarr[3] = (domainElem[i][0]);
                dataarr.push({
                x:parseInt(domainElem[i][2]),
                x2: parseInt(domainElem[i][3]),
                y:3,
                color: '#984ea3',
                key: (domainElem[i][1])
                });
                break;
            
            case "C-Terms":
                catarr[4] = (domainElem[i][0]);
                dataarr.push({
                x:parseInt(domainElem[i][2]),
                x2: parseInt(domainElem[i][3]),
                y:4,
                color: '#ff7f00',
                key: (domainElem[i][1])
                });
                break;
            
            case "Cleavages":
                catarr[5] = (domainElem[i][0]);
                dataarr.push({
                x:parseInt(domainElem[i][2]),
                x2: parseInt(domainElem[i][3]),
                y:5,
                color: '#CCCC00',
                key: (domainElem[i][1])
                });
                break;
            
            case "Modifications":
                catarr[6] = (domainElem[i][0]);
                dataarr.push({
                x:parseInt(domainElem[i][2]),
                x2: parseInt(domainElem[i][3]),
                y:6,
                color: '#a65628',
                key: (domainElem[i][1])
                });
                break;
            
            case "Variants":
                catarr[7] = (domainElem[i][0]);
                dataarr.push({
                x:parseInt(domainElem[i][2]),
                x2: parseInt(domainElem[i][3]),
                y:7,
                color: '#f781bf',
                key: (domainElem[i][1])
                });
                break;
            
            case "Domains":
                catarr[8] = (domainElem[i][0]);
                dataarr.push({
                x:parseInt(domainElem[i][2]),
                x2: parseInt(domainElem[i][3]),
                y:8,
                color: '#999999',
                key: (domainElem[i][1])
                });
                break;
            
        }
        }


    

    (function (H) {
    var defaultPlotOptions = H.getOptions().plotOptions,
    columnType = H.seriesTypes.column,
    each = H.each,
    extendClass = H.extendClass,
    pick = H.pick,
    Point = H.Point,
    Series = H.Series;
    
    defaultPlotOptions.xrange = H.merge(defaultPlotOptions.column, {
    tooltip: {
    headerFormat: '<span style="font-size: 0.85em">{point.x} - {point.x2}</span><br/>',
    pointFormat: '<span style="color:{point.color}">\u25CF</span> {point.yCategory}: <b>{point.key}</b><br/>',
    footerFormat: '<span style="color:{point.color}">{series.name}</span><br/>'
    }
    });
    H.seriesTypes.xrange = H.extendClass(columnType, {
    pointClass: extendClass(Point, {
        // Add x2 and yCategory to the available properties for tooltip formats
    getLabelConfig: function () {
        var cfg = Point.prototype.getLabelConfig.call(this);
        
        cfg.x2 = this.x2;
        cfg.yCategory = this.yCategory = this.series.yAxis.categories && this.series.yAxis.categories[this.y];
        return cfg;
    }
    }),
    type: 'xrange',
    forceDL: true,
    parallelArrays: ['x', 'x2', 'y'],
    requireSorting: false,
    animate: H.seriesTypes.line.prototype.animate,
        
        /**
         * Borrow the column series metrics, but with swapped axes. This gives free access
         * to features like groupPadding, grouping, pointWidth etc.
         */
    getColumnMetrics: function () {
        var metrics,
        chart = this.chart;
        
        function swapAxes() {
            each(chart.series, function (s) {
                var xAxis = s.xAxis;
                s.xAxis = s.yAxis;
                s.yAxis = xAxis;
            });
        }
        
        swapAxes();
        
        this.yAxis.closestPointRange = 1;
        metrics = columnType.prototype.getColumnMetrics.call(this);
        
        swapAxes();
        
        return metrics;
    },
        
        /**
         * Override cropData to show a point where x is outside visible range
         * but x2 is outside.
         */
    cropData: function (xData, yData, min, max) {
        
        // Replace xData with x2Data to find the appropriate cropStart
        var crop = Series.prototype.cropData.call(this, this.x2Data, yData, min, max);
        
        // Re-insert the cropped xData
        crop.xData = xData.slice(crop.start, crop.end);
        
        return crop;
    },
        
    translate: function () {
        columnType.prototype.translate.apply(this, arguments);
        var series = this,
        xAxis = series.xAxis,
        metrics = series.columnMetrics,
        minPointLength = series.options.minPointLength || 0;
        
        H.each(series.points, function (point) {
            var plotX = point.plotX,
            plotX2 = xAxis.toPixels(H.pick(point.x2, point.x + (point.len || 0)), true),
            width = plotX2 - plotX,
            widthDifference;
            
            if (minPointLength) {
                widthDifference = width < minPointLength ? minPointLength - width : 0;
                plotX -= widthDifference / 2;
                plotX2 += widthDifference / 2;
            }
            
            plotX = Math.max(plotX, -10);
            plotX2 = Math.min(Math.max(plotX2, -10), xAxis.len + 10);
            
            point.shapeArgs = {
            x: plotX,
            y: point.plotY + metrics.offset,
            width: plotX2 - plotX,
            height: metrics.width
            };
            point.tooltipPos[0] += width / 2;
            point.tooltipPos[1] -= metrics.width / 2;
        });
    }
    });
    
    /**
     * Max x2 should be considered in xAxis extremes
     */
    H.wrap(H.Axis.prototype, 'getSeriesExtremes', function (proceed) {
        var axis = this,
        dataMax,
        modMax;
        
        proceed.call(this);
        if (this.isXAxis) {
            dataMax = pick(axis.dataMax, Number.MIN_VALUE);
            each(this.series, function (series) {
                each(series.x2Data || [], function (val) {
                    if (val > dataMax) {
                        dataMax = val;
                        modMax = true;
                    }
                });
            });
            if (modMax) {
                axis.dataMax = dataMax;
            }
        }
    });
}(Highcharts));

Highcharts.chart('container_chart', {
    chart: {
    type: 'xrange'
    },
    title: {
    text: 'Summary'
    },
    xAxis: {
    type: 'linear'
    },
    yAxis: {
    title: {
    text: ''
    },
    categories: [catarr[0], catarr[1], catarr[2], catarr[3], catarr[4], catarr[5], catarr[6], catarr[7], catarr[8]],
    reversed: true
    },
    series: [{
    name: 'O43561',
    pointWidth: 20,
    colorByPoint: true,
    data: dataarr,
    point: {
    events: {
    }
    }
        
    }]
    });


    });

</script>


