,342],["Topology","STRAND (391-393)",391,393],["Topology","STRAND (458-460)",458,460],["Topology","STRAND (466-468)",466,468],["Topology","STRAND (478-486)",478,486],["Topology","STRAND (492-501)",492,501],["Topology","STRAND (504-507)",504,507],["Topology","STRAND (618-620)",618,620],["Topology","STRAND (732-735)",732,735],["Topology","STRAND (738-740)",738,740],["Topology","STRAND (742-746)",742,746],["Topology","HELIX (59-61)",59,61],["Topology","HELIX (76-90)",76,90],["Topology","HELIX (112-123)",112,123],["Topology","HELIX (165-175)",165,175],["Topology","HELIX (176-178)",176,178],["Topology","HELIX (190-193)",190,193],["Topology","HELIX (209-221)",209,221],["Topology","HELIX (235-250)",235,250],["Topology","HELIX (267-278)",267,278],["Topology","HELIX (293-306)",293,306],["Topology","HELIX (324-327)",324,327],["Topology","HELIX (331-334)",331,334],["Topology","HELIX (348-354)",348,354],["Topology","HELIX (368-375)",368,375],["Topology","HELIX (410-431)",410,431],["Topology","HELIX (440-442)",440,442],["Topology","HELIX (447-455)",447,455],["Topology","HELIX (591-616)",591,616],["Topology","HELIX (621-624)",621,624],["Topology","HELIX (628-649)",628,649],["Topology","HELIX (654-685)",654,685],["Topology","HELIX (703-727)",703,727],["Topology","HELIX (750-773)",750,773],["Topology","HELIX (779-807)",779,807],["Topology","HELIX (811-839)",811,839],["Topology","TURN (62-65)",62,65],["Topology","TURN (72-75)",72,75],["Topology","TURN (195-197)",195,197],["Topology","TURN (279-284)",279,284],["Topology","TURN (318-322)",318,322],["Topology","TURN (359-361)",359,361],["Topology","TURN (400-403)",400,403],["Topology","TURN (509-511)",509,511],["Topology","TURN (774-776)",774,776]];

        var dataarr = [];
        var catarr = [];
        var i;

        for (i = 0; i < domainElem.length ; i++ )
        {
        switch(domainElem[i][0]) {
            
          case "Sequence":
                catarr[0] = (domainElem[i][0]);
                dataarr.push({
                x:parseInt(domainElem[i][2]),
                x2: parseInt(domainElem[i][3]),
                y:0,
                color: '#e41a1c',
                key: (domainElem[i][1])
                });
                break;

            
            case "Chains":
                catarr[1] = (domainElem[i][0]);
                dataarr.push({
                x:parseInt(domainElem[i][2]),
                x2: parseInt(domainElem[i][3]),
                y:1,
                color: '#377eb8',
                key: (domainElem[i][1])
                });
                break;
            
            case "Topology":
                catarr[2] = (domainElem[i][0]);
                dataarr.push({
                x: parseInt(domainElem[i][2]),
                x2: parseInt(domainElem[i][3]),
                y:2,
                color: '#4daf4a',
                key: (domainElem[i][1])
                });
                break;
            
            case "N-Terms":
                catarr[3] = (domainElem[i][0]);
                dataarr.push({
                x:parseInt(domainElem[i][2]),
                x2: parseInt(domainElem[i][3]),
                y:3,
                color: '#984ea3',
                key: (domainElem[i][1])
                });
                break;
            
            case "C-Terms":
                catarr[4] = (domainElem[i][0]);
                dataarr.push({
                x:parseInt(domainElem[i][2]),
                x2: parseInt(domainElem[i][3]),
                y:4,
