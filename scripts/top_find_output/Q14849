    var dataarr = [];
    var catarr = [];


    $(document).ready(

        function makeArr() {

        var domainElem = [["Sequence","1-445",0,445],["Chains","1-445",1,445],["N-Terms","1",1,2],["C-Terms","445",444,445],["Topology","TURN (277-279)",277,279],["Topology","TURN (300-302)",300,302],["Topology","TURN (308-310)",308,310],["Topology","TURN (324-326)",324,326],["Topology","TURN (338-341)",338,341],["Topology","HELIX (233-253)",233,253],["Topology","HELIX (254-258)",254,258],["Topology","HELIX (293-299)",293,299],["Topology","HELIX (304-307)",304,307],["Topology","HELIX (416-440)",416,440],["Domains","DOMAIN (46-217)",46,217],["Domains","DOMAIN (230-443)",230,443],["Topology","TOPO_DOM (1-51)",1,51],["Topology","TOPO_DOM (73-94)",73,94],["Topology","TOPO_DOM (116-120)",116,120],["Topology","TOPO_DOM (142-148)",142,148],["Topology","TOPO_DOM (170-445)",170,445],["Topology","STRAND (260-264)",260,264],["Topology","STRAND (270-276)",270,276],["Topology","STRAND (280-291)",280,291],["Topology","STRAND (314-323)",314,323],["Topology","STRAND (327-334)",327,334],["Topology","STRAND (345-355)",345,355],["Topology","STRAND (357-366)",357,366],["Topology","STRAND (378-380)",378,380],["Topology","STRAND (385-392)",385,392],["Topology","STRAND (400-406)",400,406],["Topology","STRAND (412-414)",412,414],["Topology","TRANSMEM (52-72)",52,72],["Topology","TRANSMEM (95-115)",95,115],["Topology","TRANSMEM (121-141)",121,141],["Topology","TRANSMEM (149-169)",149,169],["Variants","VARIANT (117-117)",117,117],["Variants","VARIANT (216-216)",216,216],["Variants","VAR_SEQ (122-183)",122,183],["Variants","VAR_SEQ (126-143)",126,143],["Topology","TOPO_DOM (1-51)",1,51],["Topology","TOPO_DOM (73-94)",73,94],["Topology","TOPO_DOM (116-120)",116,120],["Topology","TOPO_DOM (142-148)",142,148],["Topology","TOPO_DOM (170-445)",170,445],["Topology","TRANSMEM (52-72)",52,72],["Topology","TRANSMEM (95-115)",95,115],["Topology","TRANSMEM (121-141)",121,141],["Topology","TRANSMEM (149-169)",149,169],["Domains","DOMAIN (46-217)",46,217],["Domains","DOMAIN (230-443)",230,443],["Domains","MOTIF (207-212)",207,212],["Modifications","MOD_RES (209-209)",209,209],["Modifications","MOD_RES (217-217)",217,217],["Modifications","MOD_RES (221-221)",221,221],["Variants","VAR_SEQ (122-183)",122,183],["Variants","VAR_SEQ (126-143)",126,143],["Variants","VARIANT (117-117)",117,117],["Variants","VARIANT (216-216)",216,216],["Variants","MUTAGEN (66-67)",66,67],["Variants","MUTAGEN (89-89)",89,89],["Variants","MUTAGEN (113-113)",113,113],["Variants","MUTAGEN (206-212)",206,212],["Variants","MUTAGEN (207-208)",207,208],["Variants","MUTAGEN (219-219)",219,219],["Variants","MUTAGEN (307-311)",307,311],["Variants","MUTAGEN (311-311)",311,311],["Topology","HELIX (233-254)",233,254],["Topology","HELIX (255-258)",255,258],["Topology","HELIX (293-300)",293,300],["Topology","HELIX (304-310)",304,310],["Topology","HELIX (416-443)",416,443],["Topology","STRAND (260-264)",260,264],["Topology","STRAND (270-276)",270,276],["Topology","STRAND (280-291)",280,291],["Topology","STRAND (314-323)",314,323],["Topology","STRAND (327-334)",327,334],["Topology","STRAND (340-342)",340,342],["Topology","STRAND (345-355)",345,355],["Topology","STRAND (357-366)",357,366],["Topology","STRAND (378-380)",378,380],["Topology","STRAND (385-392)",385,392],["Topology","STRAND (400-406)",400,406],["Topology","STRAND (412-414)",412,414],["Topology","TURN (277-279)",277,279],["Topology","TURN (324-326)",324,326]];

        var dataarr = [];
        var catarr = [];
        var i;

        for (i = 0; i < domainElem.length ; i++ )
        {
        switch(domainElem[i][0]) {
            
          case "Sequence":
                catarr[0] = (domainElem[i][0]);
                dataarr.push({
                x:parseInt(domainElem[i][2]),
                x2: parseInt(domainElem[i][3]),
                y:0,
                color: '#e41a1c',
                key: (domainElem[i][1])
                });
                break;

            
            case "Chains":
                catarr[1] = (domainElem[i][0]);
                dataarr.push({
                x:parseInt(domainElem[i][2]),
                x2: parseInt(domainElem[i][3]),
                y:1,
                color: '#377eb8',
                key: (domainElem[i][1])
                });
                break;
            
            case "Topology":
                catarr[2] = (domainElem[i][0]);
                dataarr.push({
                x: parseInt(domainElem[i][2]),
                x2: parseInt(domainElem[i][3]),
                y:2,
                color: '#4daf4a',
                key: (domainElem[i][1])
                });
                break;
            
            case "N-Terms":
                catarr[3] = (domainElem[i][0]);
                dataarr.push({
                x:parseInt(domainElem[i][2]),
                x2: parseInt(domainElem[i][3]),
                y:3,
                color: '#984ea3',
                key: (domainElem[i][1])
                });
                break;
            
            case "C-Terms":
                catarr[4] = (domainElem[i][0]);
                dataarr.push({
                x:parseInt(domainElem[i][2]),
                x2: parseInt(domainElem[i][3]),
                y:4,
                color: '#ff7f00',
                key: (domainElem[i][1])
                });
                break;
            
            case "Cleavages":
                catarr[5] = (domainElem[i][0]);
                dataarr.push({
                x:parseInt(domainElem[i][2]),
                x2: parseInt(domainElem[i][3]),
                y:5,
                color: '#CCCC00',
                key: (domainElem[i][1])
                });
                break;
            
            case "Modifications":
                catarr[6] = (domainElem[i][0]);
                dataarr.push({
                x:parseInt(domainElem[i][2]),
                x2: parseInt(domainElem[i][3]),
                y:6,
                color: '#a65628',
                key: (domainElem[i][1])
                });
                break;
            
            case "Variants":
                catarr[7] = (domainElem[i][0]);
                dataarr.push({
                x:parseInt(domainElem[i][2]),
                x2: parseInt(domainElem[i][3]),
                y:7,
                color: '#f781bf',
                key: (domainElem[i][1])
                });
                break;
            
            case "Domains":
                catarr[8] = (domainElem[i][0]);
                dataarr.push({
                x:parseInt(domainElem[i][2]),
                x2: parseInt(domainElem[i][3]),
                y:8,
                color: '#999999',
                key: (domainElem[i][1])
                });
                break;
            
        }
        }


    

    (function (H) {
    var defaultPlotOptions = H.getOptions().plotOptions,
    columnType = H.seriesTypes.column,
    each = H.each,
    extendClass = H.extendClass,
    pick = H.pick,
    Point = H.Point,
    Series = H.Series;
    
    defaultPlotOptions.xrange = H.merge(defaultPlotOptions.column, {
    tooltip: {
    headerFormat: '<span style="font-size: 0.85em">{point.x} - {point.x2}</span><br/>',
    pointFormat: '<span style="color:{point.color}">\u25CF</span> {point.yCategory}: <b>{point.key}</b><br/>',
    footerFormat: '<span style="color:{point.color}">{series.name}</span><br/>'
    }
    });
    H.seriesTypes.xrange = H.extendClass(columnType, {
    pointClass: extendClass(Point, {
        // Add x2 and yCategory to the available properties for tooltip formats
    getLabelConfig: function () {
        var cfg = Point.prototype.getLabelConfig.call(this);
        
        cfg.x2 = this.x2;
        cfg.yCategory = this.yCategory = this.series.yAxis.categories && this.series.yAxis.categories[this.y];
        return cfg;
    }
    }),
    type: 'xrange',
    forceDL: true,
    parallelArrays: ['x', 'x2', 'y'],
    requireSorting: false,
    animate: H.seriesTypes.line.prototype.animate,
        
        /**
         * Borrow the column series metrics, but with swapped axes. This gives free access
         * to features like groupPadding, grouping, pointWidth etc.
         */
    getColumnMetrics: function () {
        var metrics,
        chart = this.chart;
        
        function swapAxes() {
            each(chart.series, function (s) {
                var xAxis = s.xAxis;
                s.xAxis = s.yAxis;
                s.yAxis = xAxis;
            });
        }
        
        swapAxes();
        
        this.yAxis.closestPointRange = 1;
        metrics = columnType.prototype.getColumnMetrics.call(this);
        
        swapAxes();
        
        return metrics;
    },
        
        /**
         * Override cropData to show a point where x is outside visible range
         * but x2 is outside.
         */
    cropData: function (xData, yData, min, max) {
        
        // Replace xData with x2Data to find the appropriate cropStart
        var crop = Series.prototype.cropData.call(this, this.x2Data, yData, min, max);
        
        // Re-insert the cropped xData
        crop.xData = xData.slice(crop.start, crop.end);
        
        return crop;
    },
        
    translate: function () {
        columnType.prototype.translate.apply(this, arguments);
        var series = this,
        xAxis = series.xAxis,
        metrics = series.columnMetrics,
        minPointLength = series.options.minPointLength || 0;
        
        H.each(series.points, function (point) {
            var plotX = point.plotX,
            plotX2 = xAxis.toPixels(H.pick(point.x2, point.x + (point.len || 0)), true),
            width = plotX2 - plotX,
            widthDifference;
            
            if (minPointLength) {
                widthDifference = width < minPointLength ? minPointLength - width : 0;
                plotX -= widthDifference / 2;
                plotX2 += widthDifference / 2;
            }
            
            plotX = Math.max(plotX, -10);
            plotX2 = Math.min(Math.max(plotX2, -10), xAxis.len + 10);
            
            point.shapeArgs = {
            x: plotX,
            y: point.plotY + metrics.offset,
            width: plotX2 - plotX,
            height: metrics.width
            };
            point.tooltipPos[0] += width / 2;
            point.tooltipPos[1] -= metrics.width / 2;
        });
    }
    });
    
    /**
     * Max x2 should be considered in xAxis extremes
     */
    H.wrap(H.Axis.prototype, 'getSeriesExtremes', function (proceed) {
        var axis = this,
        dataMax,
        modMax;
        
        proceed.call(this);
        if (this.isXAxis) {
            dataMax = pick(axis.dataMax, Number.MIN_VALUE);
            each(this.series, function (series) {
                each(series.x2Data || [], function (val) {
                    if (val > dataMax) {
                        dataMax = val;
                        modMax = true;
                    }
                });
            });
            if (modMax) {
                axis.dataMax = dataMax;
            }
        }
    });
}(Highcharts));

Highcharts.chart('container_chart', {
    chart: {
    type: 'xrange'
    },
    title: {
    text: 'Summary'
    },
    xAxis: {
    type: 'linear'
    },
    yAxis: {
    title: {
    text: ''
    },
    categories: [catarr[0], catarr[1], catarr[2], catarr[3], catarr[4], catarr[5], catarr[6], catarr[7], catarr[8]],
    reversed: true
    },
    series: [{
    name: 'Q14849',
    pointWidth: 20,
    colorByPoint: true,
    data: dataarr,
    point: {
    events: {
    }
    }
        
    }]
    });


    });

</script>


