<script type="text/javascript">

    var dataarr = [];
    var catarr = [];


    $(document).ready(

        function makeArr() {

        var domainElem = [["Sequence","1-301",0,301],["Chains","1-301",1,301],["N-Terms","1",1,2],["C-Terms","301",300,301],["Domains","MOTIF (82-84)",82,84],["Domains","MOTIF (214-216)",214,216],["Variants","VAR_SEQ (236-301)",236,301],["Topology","TOPO_DOM (1-27)",1,27],["Topology","TOPO_DOM (49-52)",49,52],["Topology","TOPO_DOM (74-101)",74,101],["Topology","TOPO_DOM (123-156)",123,156],["Topology","TOPO_DOM (178-187)",178,187],["Topology","TOPO_DOM (209-301)",209,301],["Topology","TRANSMEM (28-48)",28,48],["Topology","TRANSMEM (53-73)",53,73],["Topology","TRANSMEM (102-122)",102,122],["Topology","TRANSMEM (157-177)",157,177],["Topology","TRANSMEM (188-208)",188,208],["Variants","VARIANT (15-15)",15,15],["Variants","VARIANT (123-123)",123,123],["Topology","TOPO_DOM (1-25)",1,25],["Topology","TOPO_DOM (47-52)",47,52],["Topology","TOPO_DOM (74-77)",74,77],["Topology","TOPO_DOM (92-97)",92,97],["Topology","TOPO_DOM (123-156)",123,156],["Topology","TOPO_DOM (178-187)",178,187],["Topology","TOPO_DOM (227-243)",227,243],["Topology","TOPO_DOM (265-301)",265,301],["Topology","TRANSMEM (26-46)",26,46],["Topology","TRANSMEM (53-73)",53,73],["Topology","TRANSMEM (98-122)",98,122],["Topology","TRANSMEM (157-177)",157,177],["Topology","TRANSMEM (188-207)",188,207],["Topology","TRANSMEM (244-264)",244,264],["Topology","INTRAMEM (78-91)",78,91],["Topology","INTRAMEM (208-226)",208,226],["Domains","MOTIF (82-84)",82,84],["Domains","MOTIF (214-216)",214,216],["Variants","VAR_SEQ (236-301)",236,301],["Variants","VARIANT (15-15)",15,15],["Variants","VARIANT (123-123)",123,123],["Variants","MUTAGEN (27-27)",27,27],["Variants","MUTAGEN (73-73)",73,73],["Variants","MUTAGEN (73-73)",73,73],["Variants","MUTAGEN (77-77)",77,77],["Variants","MUTAGEN (80-80)",80,80],["Variants","MUTAGEN (85-85)",85,85],["Variants","MUTAGEN (94-94)",94,94],["Variants","MUTAGEN (133-133)",133,133],["Topology","HELIX (19-47)",19,47],["Topology","HELIX (55-72)",55,72],["Topology","HELIX (83-91)",83,91],["Topology","HELIX (97-99)",97,99],["Topology","HELIX (100-121)",100,121],["Topology","HELIX (123-130)",123,130],["Topology","HELIX (143-145)",143,145],["Topology","HELIX (156-178)",156,178],["Topology","HELIX (190-205)",190,205],["Topology","HELIX (215-227)",215,227],["Topology","HELIX (233-240)",233,240],["Topology","HELIX (241-243)",241,243],["Topology","HELIX (244-267)",244,267],["Topology","TURN (48-50)",48,50],["Topology","TURN (207-209)",207,209],["Topology","TURN (228-230)",228,230],["Topology","STRAND (136-138)",136,138]];

        var dataarr = [];
        var catarr = [];
        var i;

        for (i = 0; i < domainElem.length ; i++ )
        {
        switch(domainElem[i][0]) {
            
          case "Sequence":
                catarr[0] = (domainElem[i][0]);
                dataarr.push({
                x:parseInt(domainElem[i][2]),
                x2: parseInt(domainElem[i][3]),
                y:0,
                color: '#e41a1c',
                key: (domainElem[i][1])
                });
                break;

            
            case "Chains":
                catarr[1] = (domainElem[i][0]);
                dataarr.push({
                x:parseInt(domainElem[i][2]),
                x2: parseInt(domainElem[i][3]),
                y:1,
                color: '#377eb8',
                key: (domainElem[i][1])
                });
                break;
            
            case "Topology":
                catarr[2] = (domainElem[i][0]);
                dataarr.push({
                x: parseInt(domainElem[i][2]),
                x2: parseInt(domainElem[i][3]),
                y:2,
                color: '#4daf4a',
                key: (domainElem[i][1])
                });
                break;
            
            case "N-Terms":
                catarr[3] = (domainElem[i][0]);
                dataarr.push({
                x:parseInt(domainElem[i][2]),
                x2: parseInt(domainElem[i][3]),
                y:3,
                color: '#984ea3',
                key: (domainElem[i][1])
                });
                break;
            
            case "C-Terms":
                catarr[4] = (domainElem[i][0]);
                dataarr.push({
                x:parseInt(domainElem[i][2]),
                x2: parseInt(domainElem[i][3]),
                y:4,
                color: '#ff7f00',
                key: (domainElem[i][1])
                });
                break;
            
            case "Cleavages":
                catarr[5] = (domainElem[i][0]);
                dataarr.push({
                x:parseInt(domainElem[i][2]),
                x2: parseInt(domainElem[i][3]),
                y:5,
                color: '#CCCC00',
                key: (domainElem[i][1])
                });
                break;
            
            case "Modifications":
                catarr[6] = (domainElem[i][0]);
                dataarr.push({
                x:parseInt(domainElem[i][2]),
                x2: parseInt(domainElem[i][3]),
                y:6,
                color: '#a65628',
                key: (domainElem[i][1])
                });
                break;
            
            case "Variants":
                catarr[7] = (domainElem[i][0]);
                dataarr.push({
                x:parseInt(domainElem[i][2]),
                x2: parseInt(domainElem[i][3]),
                y:7,
                color: '#f781bf',
                key: (domainElem[i][1])
                });
                break;
            
            case "Domains":
                catarr[8] = (domainElem[i][0]);
                dataarr.push({
                x:parseInt(domainElem[i][2]),
                x2: parseInt(domainElem[i][3]),
                y:8,
                color: '#999999',
                key: (domainElem[i][1])
                });
                break;
            
        }
        }


    

    (function (H) {
    var defaultPlotOptions = H.getOptions().plotOptions,
    columnType = H.seriesTypes.column,
    each = H.each,
    extendClass = H.extendClass,
    pick = H.pick,
    Point = H.Point,
    Series = H.Series;
    
    defaultPlotOptions.xrange = H.merge(defaultPlotOptions.column, {
    tooltip: {
    headerFormat: '<span style="font-size: 0.85em">{point.x} - {point.x2}</span><br/>',
    pointFormat: '<span style="color:{point.color}">\u25CF</span> {point.yCategory}: <b>{point.key}</b><br/>',
    footerFormat: '<span style="color:{point.color}">{series.name}</span><br/>'
    }
    });
    H.seriesTypes.xrange = H.extendClass(columnType, {
    pointClass: extendClass(Point, {
        // Add x2 and yCategory to the available properties for tooltip formats
    getLabelConfig: function () {
        var cfg = Point.prototype.getLabelConfig.call(this);
        
        cfg.x2 = this.x2;
        cfg.yCategory = this.yCategory = this.series.yAxis.categories && this.series.yAxis.categories[this.y];
        return cfg;
    }
    }),
    type: 'xrange',
    forceDL: true,
    parallelArrays: ['x', 'x2', 'y'],
    requireSorting: false,
    animate: H.seriesTypes.line.prototype.animate,
        
        /**
         * Borrow the column series metrics, but with swapped axes. This gives free access
         * to features like groupPadding, grouping, pointWidth etc.
         */
    getColumnMetrics: function () {
        var metrics,
        chart = this.chart;
        
        function swapAxes() {
            each(chart.series, function (s) {
                var xAxis = s.xAxis;
                s.xAxis = s.yAxis;
                s.yAxis = xAxis;
            });
        }
        
        swapAxes();
        
        this.yAxis.closestPointRange = 1;
        metrics = columnType.prototype.getColumnMetrics.call(this);
        
        swapAxes();
        
        return metrics;
    },
        
        /**
         * Override cropData to show a point where x is outside visible range
         * but x2 is outside.
         */
    cropData: function (xData, yData, min, max) {
        
        // Replace xData with x2Data to find the appropriate cropStart
        var crop = Series.prototype.cropData.call(this, this.x2Data, yData, min, max);
        
        // Re-insert the cropped xData
        crop.xData = xData.slice(crop.start, crop.end);
        
        return crop;
    },
        
    translate: function () {
        columnType.prototype.translate.apply(this, arguments);
        var series = this,
        xAxis = series.xAxis,
        metrics = series.columnMetrics,
        minPointLength = series.options.minPointLength || 0;
        
        H.each(series.points, function (point) {
            var plotX = point.plotX,
            plotX2 = xAxis.toPixels(H.pick(point.x2, point.x + (point.len || 0)), true),
            width = plotX2 - plotX,
            widthDifference;
            
            if (minPointLength) {
                widthDifference = width < minPointLength ? minPointLength - width : 0;
                plotX -= widthDifference / 2;
                plotX2 += widthDifference / 2;
            }
            
            plotX = Math.max(plotX, -10);
            plotX2 = Math.min(Math.max(plotX2, -10), xAxis.len + 10);
            
            point.shapeArgs = {
            x: plotX,
            y: point.plotY + metrics.offset,
            width: plotX2 - plotX,
            height: metrics.width
            };
            point.tooltipPos[0] += width / 2;
            point.tooltipPos[1] -= metrics.width / 2;
        });
    }
    });
    
    /**
     * Max x2 should be considered in xAxis extremes
     */
    H.wrap(H.Axis.prototype, 'getSeriesExtremes', function (proceed) {
        var axis = this,
        dataMax,
        modMax;
        
        proceed.call(this);
        if (this.isXAxis) {
            dataMax = pick(axis.dataMax, Number.MIN_VALUE);
            each(this.series, function (series) {
                each(series.x2Data || [], function (val) {
                    if (val > dataMax) {
                        dataMax = val;
                        modMax = true;
                    }
                });
            });
            if (modMax) {
                axis.dataMax = dataMax;
            }
        }
    });
}(Highcharts));

Highcharts.chart('container_chart', {
    chart: {
    type: 'xrange'
    },
    title: {
    text: 'Summary'
    },
    xAxis: {
    type: 'linear'
    },
    yAxis: {
    title: {
    text: ''
    },
    categories: [catarr[0], catarr[1], catarr[2], catarr[3], catarr[4], catarr[5], catarr[6], catarr[7], catarr[8]],
    reversed: true
    },
    series: [{
    name: 'Q96PS8',
    pointWidth: 20,
    colorByPoint: true,
    data: dataarr,
    point: {
    events: {
    }
    }
        
    }]
    });


    });

</script>


