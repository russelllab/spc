3],["Topology","TRANSMEM (794-819)",794,819],["Domains","REGION (166-168)",166,168],["Domains","REGION (677-685)",677,685],["Features","BINDING (145-145)",145,145],["Features","BINDING (216-216)",216,216],["Features","BINDING (295-295)",295,295],["Features","BINDING (377-377)",377,377],["Variants","MUTAGEN (677-677)",677,677],["Variants","MUTAGEN (681-681)",681,681],["Variants","MUTAGEN (685-685)",685,685],["Variants","CONFLICT (12-12)",12,12],["Variants","CONFLICT (210-210)",210,210],["Variants","CONFLICT (496-496)",496,496],["Variants","CONFLICT (748-748)",748,748],["Variants","CONFLICT (776-776)",776,776],["Topology","STRAND (26-28)",26,28],["Topology","STRAND (31-38)",31,38],["Topology","STRAND (41-43)",41,43],["Topology","STRAND (49-53)",49,53],["Topology","STRAND (76-81)",76,81],["Topology","STRAND (84-90)",84,90],["Topology","STRAND (138-140)",138,140],["Topology","STRAND (162-166)",162,166],["Topology","STRAND (181-185)",181,185],["Topology","STRAND (206-214)",206,214],["Topology","STRAND (234-241)",234,241],["Topology","STRAND (260-262)",260,262],["Topology","STRAND (265-269)",265,269],["Topology","STRAND (290-293)",290,293],["Topology","STRAND (315-320)",315,320],["Topology","STRAND (357-359)",357,359],["Topology","STRAND (427-429)",427,429],["Topology","STRAND (440-442)",440,442],["Topology","STRAND (453-460)",453,460],["Topology","STRAND (466-480)",466,480],["Topology","STRAND (508-511)",508,511],["Topology","STRAND (521-524)",521,524],["Topology","STRAND (529-533)",529,533],["Topology","STRAND (536-539)",536,539],["Topology","TURN (55-58)",55,58],["Topology","TURN (107-109)",107,109],["Topology","TURN (175-177)",175,177],["Topology","TURN (295-299)",295,299],["Topology","TURN (336-338)",336,338],["Topology","TURN (361-364)",361,364],["Topology","TURN (367-369)",367,369],["Topology","TURN (486-490)",486,490],["Topology","TURN (548-550)",548,550],["Topology","HELIX (59-74)",59,74],["Topology","HELIX (95-106)",95,106],["Topology","HELIX (145-155)",145,155]