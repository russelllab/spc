        var i;

        for (i = 0; i < domainElem.length ; i++ )
        {
        switch(domainElem[i][0]) {
            
          case "Sequence":
                catarr[0] = (domainElem[i][0]);
                dataarr.push({
                x:parseInt(domainElem[i][2]),
                x2: parseInt(domainElem[i][3]),
                y:0,
                color: '#e41a1c',
                key: (domainElem[i][1])
                });
                break;

            
            case "Chains":
                catarr[1] = (domainElem[i][0]);
                dataarr.push({
                x:parseInt(domainElem[i][2]),
                x2: parseInt(domainElem[i][3]),
