gy","HELIX (440-455)",440,455],["Variants","VARIANT (39-39)",39,39],["Variants","VARIANT (201-201)",201,201],["Variants","VARIANT (264-264)",264,264],["Variants","VARIANT (365-365)",365,365],["Variants","VARIANT (375-375)",375,375],["Variants","VARIANT (375-375)",375,375],["Variants","VARIANT (435-435)",435,435],["Variants","VARIANT (437-437)",437,437],["Features","METAL (437-437)",437,437],["Features","BINDING (309-309)",309,309],["Features","BINDING (374-374)",374,374],["Variants","VAR_SEQ (210-218)",210,218],["Variants","VAR_SEQ (219-503)",219,503],["Variants","VARIANT (39-39)",39,39],["Variants","VARIANT (192-192)",192,192],["Variants","VARIANT (201-201)",201,201],["Variants","VARIANT (264-264)",264,264],["Variants","VARIANT (264-264)",264,264],["Variants","VARIANT (314-314)",314,314],["Variants","VARIANT (365-365)",365,365],["Variants","VARIANT (375-375)",375,375],["Variants","VARIANT (375-375)",375,375],["Variants","VARIANT (435-435)",435,435],["Variants","VARIANT (437-437)",437,437],["Variants","CONFLICT (496-496)",496,496],["Topology","STRAND (54-56)",54,56],["Topology","STRAND (83-97)",83,97],["Topology","STRAND (130-132)",130,132],["Topology","STRAND (183-185)",183,185],["Topology","STRAND (373-376)",373,376],["Topology","STRAND (381-383)",381,383],["Topology","STRAND (386-388)",386,388],["Topology","STRAND (393-396)",393,396],["Topology","STRAND (458-463)",458,463],["Topology","STRAND (473-481)",473,481],["Topology","STRAND (483-485)",483,485],["Topology","STRAND (490-494)",490,494],["Topology","HELIX (57-68)",57,68],["Topology","HELIX (70-80)",70,80],["Topology","HELIX (100-108)",100,108],["Topology","HELIX (110-112)",110,112],["Topology","HELIX (119-125)",119,125],["Topology","HELIX (138-151)",138,151],["Topology","HELIX (155-172)",155,172],["Topology","HELIX (173-177)",173,177],["Topology","HELIX (187-203)",187,203],["Topology","HELIX (210-227)",210,227],["Topology","HELIX (232-236)",232,236],["Topology","HELIX (238-240)",238,240],["Topology","HELIX (2