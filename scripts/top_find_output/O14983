<script type="text/javascript">

    var dataarr = [];
    var catarr = [];


    $(document).ready(

        function makeArr() {

        var domainElem = [["Sequence","1-1001",0,1001],["Chains","1-1001",1,1001],["N-Terms","1",1,2],["N-Terms","126",126,127],["C-Terms","141",140,141],["C-Terms","151",150,151],["C-Terms","1001",1000,1001],["Variants","VAR_SEQ (1-125)",1,125],["Variants","VAR_SEQ (994-1001)",994,1001],["Topology","TOPO_DOM (1-48)",1,48],["Topology","TOPO_DOM (70-89)",70,89],["Topology","TOPO_DOM (111-253)",111,253],["Topology","TOPO_DOM (274-295)",274,295],["Topology","TOPO_DOM (314-757)",314,757],["Topology","TOPO_DOM (778-787)",778,787],["Topology","TOPO_DOM (809-828)",809,828],["Topology","TOPO_DOM (852-897)",852,897],["Topology","TOPO_DOM (918-930)",918,930],["Topology","TOPO_DOM (950-964)",950,964],["Topology","TOPO_DOM (986-1001)",986,1001],["Topology","TRANSMEM (49-69)",49,69],["Topology","TRANSMEM (90-110)",90,110],["Topology","TRANSMEM (254-273)",254,273],["Topology","TRANSMEM (296-313)",296,313],["Topology","TRANSMEM (758-777)",758,777],["Topology","TRANSMEM (788-808)",788,808],["Topology","TRANSMEM (829-851)",829,851],["Topology","TRANSMEM (898-917)",898,917],["Topology","TRANSMEM (931-949)",931,949],["Topology","TRANSMEM (965-985)",965,985],["Features","ACT_SITE (351-351)",351,351],["Features","METAL (304-304)",304,304],["Features","METAL (305-305)",305,305],["Features","METAL (307-307)",307,307],["Features","METAL (309-309)",309,309],["Features","METAL (703-703)",703,703],["Features","METAL (707-707)",707,707],["Features","METAL (768-768)",768,768],["Features","METAL (771-771)",771,771],["Features","METAL (796-796)",796,796],["Features","METAL (799-799)",799,799],["Features","METAL (800-800)",800,800],["Features","METAL (800-800)",800,800],["Features","METAL (908-908)",908,908],["Variants","VARIANT (789-789)",789,789],["Domains","REGION (370-400)",370,400],["Domains","REGION (788-808)",788,808],["Topology","TRANSMEM (49-69)",49,69],["Topology","TRANSMEM (90-110)",90,110],["Topology","TRANSMEM (254-273)",254,273],["Topology","TRANSMEM (296-313)",296,313],["Topology","TRANSMEM (758-777)",758,777],["Topology","TRANSMEM (788-808)",788,808],["Topology","TRANSMEM (829-851)",829,851],["Topology","TRANSMEM (898-917)",898,917],["Topology","TRANSMEM (931-949)",931,949],["Topology","TRANSMEM (965-985)",965,985],["Features","NP_BIND (625-627)",625,627],["Domains","REGION (788-808)",788,808],["Domains","REGION (932-943)",932,943],["Features","ACT_SITE (351-351)",351,351],["Features","METAL (304-304)",304,304],["Features","METAL (305-305)",305,305],["Features","METAL (307-307)",307,307],["Features","METAL (309-309)",309,309],["Features","METAL (351-351)",351,351],["Features","METAL (353-353)",353,353],["Features","METAL (703-703)",703,703],["Features","METAL (768-768)",768,768],["Features","METAL (771-771)",771,771],["Features","METAL (796-796)",796,796],["Features","METAL (799-799)",799,799],["Features","METAL (800-800)",800,800],["Features","METAL (800-800)",800,800],["Features","METAL (908-908)",908,908],["Features","BINDING (353-353)",353,353],["Features","BINDING (442-442)",442,442],["Features","BINDING (489-489)",489,489],["Features","BINDING (515-515)",515,515],["Features","BINDING (560-560)",560,560],["Features","BINDING (678-678)",678,678],["Features","BINDING (684-684)",684,684],["Features","BINDING (706-706)",706,706],["Modifications","MOD_RES (441-441)",441,441],["Modifications","MOD_RES (569-569)",569,569],["Modifications","MOD_RES (581-581)",581,581],["Variants","VAR_SEQ (1-125)",1,125],["Variants","VAR_SEQ (994-1001)",994,1001],["Variants","VARIANT (789-789)",789,789]];

        var dataarr = [];
        var catarr = [];
        var i;

        for (i = 0; i < domainElem.length ; i++ )
        {
        switch(domainElem[i][0]) {
            
          case "Sequence":
                catarr[0] = (domainElem[i][0]);
                dataarr.push({
                x:parseInt(domainElem[i][2]),
                x2: parseInt(domainElem[i][3]),
                y:0,
                color: '#e41a1c',
                key: (domainElem[i][1])
                });
                break;

            
            case "Chains":
                catarr[1] = (domainElem[i][0]);
                dataarr.push({
                x:parseInt(domainElem[i][2]),
                x2: parseInt(domainElem[i][3]),
                y:1,
                color: '#377eb8',
                key: (domainElem[i][1])
                });
                break;
            
            case "Topology":
                catarr[2] = (domainElem[i][0]);
                dataarr.push({
                x: parseInt(domainElem[i][2]),
                x2: parseInt(domainElem[i][3]),
                y:2,
                color: '#4daf4a',
                key: (domainElem[i][1])
                });
                break;
            
            case "N-Terms":
                catarr[3] = (domainElem[i][0]);
                dataarr.push({
                x:parseInt(domainElem[i][2]),
                x2: parseInt(domainElem[i][3]),
                y:3,
                color: '#984ea3',
                key: (domainElem[i][1])
                });
                break;
            
            case "C-Terms":
                catarr[4] = (domainElem[i][0]);
                dataarr.push({
                x:parseInt(domainElem[i][2]),
                x2: parseInt(domainElem[i][3]),
                y:4,
                color: '#ff7f00',
                key: (domainElem[i][1])
                });
                break;
            
            case "Cleavages":
                catarr[5] = (domainElem[i][0]);
                dataarr.push({
                x:parseInt(domainElem[i][2]),
                x2: parseInt(domainElem[i][3]),
                y:5,
                color: '#CCCC00',
                key: (domainElem[i][1])
                });
                break;
            
            case "Modifications":
                catarr[6] = (domainElem[i][0]);
                dataarr.push({
                x:parseInt(domainElem[i][2]),
                x2: parseInt(domainElem[i][3]),
                y:6,
                color: '#a65628',
                key: (domainElem[i][1])
                });
                break;
            
            case "Variants":
                catarr[7] = (domainElem[i][0]);
                dataarr.push({
                x:parseInt(domainElem[i][2]),
                x2: parseInt(domainElem[i][3]),
                y:7,
                color: '#f781bf',
                key: (domainElem[i][1])
                });
                break;
            
            case "Domains":
                catarr[8] = (domainElem[i][0]);
                dataarr.push({
                x:parseInt(domainElem[i][2]),
                x2: parseInt(domainElem[i][3]),
                y:8,
                color: '#999999',
                key: (domainElem[i][1])
                });
                break;
            
        }
        }


    

    (function (H) {
    var defaultPlotOptions = H.getOptions().plotOptions,
    columnType = H.seriesTypes.column,
    each = H.each,
    extendClass = H.extendClass,
    pick = H.pick,
    Point = H.Point,
    Series = H.Series;
    
    defaultPlotOptions.xrange = H.merge(defaultPlotOptions.column, {
    tooltip: {
    headerFormat: '<span style="font-size: 0.85em">{point.x} - {point.x2}</span><br/>',
    pointFormat: '<span style="color:{point.color}">\u25CF</span> {point.yCategory}: <b>{point.key}</b><br/>',
    footerFormat: '<span style="color:{point.color}">{series.name}</span><br/>'
    }
    });
    H.seriesTypes.xrange = H.extendClass(columnType, {
    pointClass: extendClass(Point, {
        // Add x2 and yCategory to the available properties for tooltip formats
    getLabelConfig: function () {
        var cfg = Point.prototype.getLabelConfig.call(this);
        
        cfg.x2 = this.x2;
        cfg.yCategory = this.yCategory = this.series.yAxis.categories && this.series.yAxis.categories[this.y];
        return cfg;
    }
    }),
    type: 'xrange',
    forceDL: true,
    parallelArrays: ['x', 'x2', 'y'],
    requireSorting: false,
    animate: H.seriesTypes.line.prototype.animate,
        
        /**
         * Borrow the column series metrics, but with swapped axes. This gives free access
         * to features like groupPadding, grouping, pointWidth etc.
         */
    getColumnMetrics: function () {
        var metrics,
        chart = this.chart;
        
        function swapAxes() {
            each(chart.series, function (s) {
                var xAxis = s.xAxis;
                s.xAxis = s.yAxis;
                s.yAxis = xAxis;
            });
        }
        
        swapAxes();
        
        this.yAxis.closestPointRange = 1;
        metrics = columnType.prototype.getColumnMetrics.call(this);
        
        swapAxes();
        
        return metrics;
    },
        
        /**
         * Override cropData to show a point where x is outside visible range
         * but x2 is outside.
         */
    cropData: function (xData, yData, min, max) {
        
        // Replace xData with x2Data to find the appropriate cropStart
        var crop = Series.prototype.cropData.call(this, this.x2Data, yData, min, max);
        
        // Re-insert the cropped xData
        crop.xData = xData.slice(crop.start, crop.end);
        
        return crop;
    },
        
    translate: function () {
        columnType.prototype.translate.apply(this, arguments);
        var series = this,
        xAxis = series.xAxis,
        metrics = series.columnMetrics,
        minPointLength = series.options.minPointLength || 0;
        
        H.each(series.points, function (point) {
            var plotX = point.plotX,
            plotX2 = xAxis.toPixels(H.pick(point.x2, point.x + (point.len || 0)), true),
            width = plotX2 - plotX,
            widthDifference;
            
            if (minPointLength) {
                widthDifference = width < minPointLength ? minPointLength - width : 0;
                plotX -= widthDifference / 2;
                plotX2 += widthDifference / 2;
            }
            
            plotX = Math.max(plotX, -10);
            plotX2 = Math.min(Math.max(plotX2, -10), xAxis.len + 10);
            
            point.shapeArgs = {
            x: plotX,
            y: point.plotY + metrics.offset,
            width: plotX2 - plotX,
            height: metrics.width
            };
            point.tooltipPos[0] += width / 2;
            point.tooltipPos[1] -= metrics.width / 2;
        });
    }
    });
    
    /**
     * Max x2 should be considered in xAxis extremes
     */
    H.wrap(H.Axis.prototype, 'getSeriesExtremes', function (proceed) {
        var axis = this,
        dataMax,
        modMax;
        
        proceed.call(this);
        if (this.isXAxis) {
            dataMax = pick(axis.dataMax, Number.MIN_VALUE);
            each(this.series, function (series) {
                each(series.x2Data || [], function (val) {
                    if (val > dataMax) {
                        dataMax = val;
                        modMax = true;
                    }
                });
            });
            if (modMax) {
                axis.dataMax = dataMax;
            }
        }
    });
}(Highcharts));

Highcharts.chart('container_chart', {
    chart: {
    type: 'xrange'
    },
    title: {
    text: 'Summary'
    },
    xAxis: {
    type: 'linear'
    },
    yAxis: {
    title: {
    text: ''
    },
    categories: [catarr[0], catarr[1], catarr[2], catarr[3], catarr[4], catarr[5], catarr[6], catarr[7], catarr[8]],
    reversed: true
    },
    series: [{
    name: 'O14983',
    pointWidth: 20,
    colorByPoint: true,
    data: dataarr,
    point: {
    events: {
    }
    }
        
    }]
    });


    });

</script>


