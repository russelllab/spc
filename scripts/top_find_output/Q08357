)",108,109],["Topology","TOPO_DOM (131-142)",131,142],["Topology","TOPO_DOM (164-190)",164,190],["Topology","TOPO_DOM (212-213)",212,213],["Topology","TOPO_DOM (235-482)",235,482],["Topology","TOPO_DOM (504-530)",504,530],["Topology","TOPO_DOM (552-571)",552,571],["Topology","TOPO_DOM (587-593)",587,593],["Topology","TOPO_DOM (610-621)",610,621],["Topology","TOPO_DOM (643-652)",643,652],["Topology","TRANSMEM (6-26)",6,26],["Topology","TRANSMEM (47-67)",47,67],["Topology","TRANSMEM (87-107)",87,107],["Topology","TRANSMEM (110-130)",110,130],["Topology","TRANSMEM (143-163)",143,163],["Topology","TRANSMEM (191-211)",191,211],["Topology","TRANSMEM (214-234)",214,234],["Topology","TRANSMEM (483-503)",483,503],["Topology","TRANSMEM (531-551)",531,551],["Topology","TRANSMEM (572-586)",572,586],["Topology","TRANSMEM (594-609)",594,609],["Topology","TRANSMEM (622-642)",622,642],["Modifications","MOD_RES (253-253)",253,253],["Modifications","MOD_RES (256-256)",256,256],["Modifications","MOD_RES (259-259)",259,259],["Modifications","MOD_RES (268-268)",268,268],["Modifications","MOD_RES (316-316)",316,316],["Modifications","MOD_RES (385-385)",385,385],["Variants","VARIANT (11-11)",11,11],["Variants","VARIANT (28-28)",28,28],["Variants","VARIANT (42-42)",42,42],["Variants","VARIANT (51-51)",51,51],["Variants","VARIANT (62-62)",62,62],["Variants","VARIANT (71-71)",71,71],["Variants","VARIANT (115-115)",115,115],["Variants","VARIANT (184-184)",184,184],["Variants","VARIANT (194-194)",194,194],["Variants","VARIANT (382-382)",382,382],["Variants","VARIANT (434-434)",434,434],["Variants","VARIANT (498-498)",498,498],["Variants","VARIANT (502-502)",502,502],["Variants","VARIANT (568-568)",568,568],["Variants","VARIANT (571-571)",571,571],["Variants","VARIANT (575-575)",575,575],["Variants","VARIANT (595-595)",595,595],["Variants","VARIANT (601-601)",601,601],["Variants","VARIANT (601-601)",601,601],["Variants","VARIANT (637-637)",637,637],["Variants","MUTAGEN (55-55)",55,55],["Variant