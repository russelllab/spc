<script type="text/javascript">

    var dataarr = [];
    var catarr = [];


    $(document).ready(

        function makeArr() {

        var domainElem = [["Sequence","1-672",0,672],["Chains","1-672",1,672],["N-Terms","1",1,2],["N-Terms","2",2,3],["N-Terms","282",282,283],["N-Terms","549",549,550],["N-Terms","549",549,550],["C-Terms","672",671,672],["Variants","VAR_SEQ (1-281)",1,281],["Variants","VAR_SEQ (140-310)",140,310],["Variants","VAR_SEQ (249-267)",249,267],["Variants","VAR_SEQ (268-672)",268,672],["Variants","CONFLICT (270-270)",270,270],["Variants","CONFLICT (456-456)",456,456],["Variants","CONFLICT (545-545)",545,545],["Topology","TOPO_DOM (1-17)",1,17],["Topology","TOPO_DOM (39-58)",39,58],["Topology","TOPO_DOM (80-102)",80,102],["Topology","TOPO_DOM (124-132)",124,132],["Topology","TOPO_DOM (154-672)",154,672],["Topology","TRANSMEM (18-38)",18,38],["Topology","TRANSMEM (59-79)",59,79],["Topology","TRANSMEM (103-123)",103,123],["Topology","TRANSMEM (133-153)",133,153],["Modifications","MOD_RES (570-570)",570,570],["Modifications","MOD_RES (574-574)",574,574],["Modifications","MOD_RES (579-579)",579,579],["Modifications","MOD_RES (584-584)",584,584],["Features","ACT_SITE (443-443)",443,443],["Features","ACT_SITE (495-495)",495,495],["Variants","MUTAGEN (443-443)",443,443],["Variants","MUTAGEN (495-495)",495,495],["Variants","VARIANT (664-664)",664,664],["Topology","TOPO_DOM (1-17)",1,17],["Topology","TOPO_DOM (39-58)",39,58],["Topology","TOPO_DOM (80-102)",80,102],["Topology","TOPO_DOM (124-132)",124,132],["Topology","TOPO_DOM (154-672)",154,672],["Topology","TRANSMEM (18-38)",18,38],["Topology","TRANSMEM (59-79)",59,79],["Topology","TRANSMEM (103-123)",103,123],["Topology","TRANSMEM (133-153)",133,153],["Features","ACT_SITE (443-443)",443,443],["Features","ACT_SITE (495-495)",495,495],["Modifications","MOD_RES (570-570)",570,570],["Modifications","MOD_RES (574-574)",574,574],["Modifications","MOD_RES (579-579)",579,579],["Modifications","MOD_RES (583-583)",583,583],["Modifications","MOD_RES (584-584)",584,584],["Variants","VAR_SEQ (1-281)",1,281],["Variants","VAR_SEQ (140-310)",140,310],["Variants","VAR_SEQ (249-267)",249,267],["Variants","VAR_SEQ (268-672)",268,672],["Variants","VARIANT (664-664)",664,664],["Variants","MUTAGEN (443-443)",443,443],["Variants","MUTAGEN (495-495)",495,495],["Variants","CONFLICT (270-270)",270,270],["Variants","CONFLICT (456-456)",456,456],["Variants","CONFLICT (545-545)",545,545]];

        var dataarr = [];
        var catarr = [];
        var i;

        for (i = 0; i < domainElem.length ; i++ )
        {
        switch(domainElem[i][0]) {
            
          case "Sequence":
                catarr[0] = (domainElem[i][0]);
                dataarr.push({
                x:parseInt(domainElem[i][2]),
                x2: parseInt(domainElem[i][3]),
                y:0,
                color: '#e41a1c',
                key: (domainElem[i][1])
                });
                break;

            
            case "Chains":
                catarr[1] = (domainElem[i][0]);
                dataarr.push({
                x:parseInt(domainElem[i][2]),
                x2: parseInt(domainElem[i][3]),
                y:1,
                color: '#377eb8',
                key: (domainElem[i][1])
                });
                break;
            
            case "Topology":
                catarr[2] = (domainElem[i][0]);
                dataarr.push({
                x: parseInt(domainElem[i][2]),
                x2: parseInt(domainElem[i][3]),
                y:2,
                color: '#4daf4a',
                key: (domainElem[i][1])
                });
                break;
            
            case "N-Terms":
                catarr[3] = (domainElem[i][0]);
                dataarr.push({
                x:parseInt(domainElem[i][2]),
                x2: parseInt(domainElem[i][3]),
                y:3,
                color: '#984ea3',
                key: (domainElem[i][1])
                });
                break;
            
            case "C-Terms":
                catarr[4] = (domainElem[i][0]);
                dataarr.push({
                x:parseInt(domainElem[i][2]),
                x2: parseInt(domainElem[i][3]),
                y:4,
                color: '#ff7f00',
                key: (domainElem[i][1])
                });
                break;
            
            case "Cleavages":
                catarr[5] = (domainElem[i][0]);
                dataarr.push({
                x:parseInt(domainElem[i][2]),
                x2: parseInt(domainElem[i][3]),
                y:5,
                color: '#CCCC00',
                key: (domainElem[i][1])
                });
                break;
            
            case "Modifications":
                catarr[6] = (domainElem[i][0]);
                dataarr.push({
                x:parseInt(domainElem[i][2]),
                x2: parseInt(domainElem[i][3]),
                y:6,
                color: '#a65628',
                key: (domainElem[i][1])
                });
                break;
            
            case "Variants":
                catarr[7] = (domainElem[i][0]);
                dataarr.push({
                x:parseInt(domainElem[i][2]),
                x2: parseInt(domainElem[i][3]),
                y:7,
                color: '#f781bf',
                key: (domainElem[i][1])
                });
                break;
            
            case "Domains":
                catarr[8] = (domainElem[i][0]);
                dataarr.push({
                x:parseInt(domainElem[i][2]),
                x2: parseInt(domainElem[i][3]),
                y:8,
                color: '#999999',
                key: (domainElem[i][1])
                });
                break;
            
        }
        }


    

    (function (H) {
    var defaultPlotOptions = H.getOptions().plotOptions,
    columnType = H.seriesTypes.column,
    each = H.each,
    extendClass = H.extendClass,
    pick = H.pick,
    Point = H.Point,
    Series = H.Series;
    
    defaultPlotOptions.xrange = H.merge(defaultPlotOptions.column, {
    tooltip: {
    headerFormat: '<span style="font-size: 0.85em">{point.x} - {point.x2}</span><br/>',
    pointFormat: '<span style="color:{point.color}">\u25CF</span> {point.yCategory}: <b>{point.key}</b><br/>',
    footerFormat: '<span style="color:{point.color}">{series.name}</span><br/>'
    }
    });
    H.seriesTypes.xrange = H.extendClass(columnType, {
    pointClass: extendClass(Point, {
        // Add x2 and yCategory to the available properties for tooltip formats
    getLabelConfig: function () {
        var cfg = Point.prototype.getLabelConfig.call(this);
        
        cfg.x2 = this.x2;
        cfg.yCategory = this.yCategory = this.series.yAxis.categories && this.series.yAxis.categories[this.y];
        return cfg;
    }
    }),
    type: 'xrange',
    forceDL: true,
    parallelArrays: ['x', 'x2', 'y'],
    requireSorting: false,
    animate: H.seriesTypes.line.prototype.animate,
        
        /**
         * Borrow the column series metrics, but with swapped axes. This gives free access
         * to features like groupPadding, grouping, pointWidth etc.
         */
    getColumnMetrics: function () {
        var metrics,
        chart = this.chart;
        
        function swapAxes() {
            each(chart.series, function (s) {
                var xAxis = s.xAxis;
                s.xAxis = s.yAxis;
                s.yAxis = xAxis;
            });
        }
        
        swapAxes();
        
        this.yAxis.closestPointRange = 1;
        metrics = columnType.prototype.getColumnMetrics.call(this);
        
        swapAxes();
        
        return metrics;
    },
        
        /**
         * Override cropData to show a point where x is outside visible range
         * but x2 is outside.
         */
    cropData: function (xData, yData, min, max) {
        
        // Replace xData with x2Data to find the appropriate cropStart
        var crop = Series.prototype.cropData.call(this, this.x2Data, yData, min, max);
        
        // Re-insert the cropped xData
        crop.xData = xData.slice(crop.start, crop.end);
        
        return crop;
    },
        
    translate: function () {
        columnType.prototype.translate.apply(this, arguments);
        var series = this,
        xAxis = series.xAxis,
        metrics = series.columnMetrics,
        minPointLength = series.options.minPointLength || 0;
        
        H.each(series.points, function (point) {
            var plotX = point.plotX,
            plotX2 = xAxis.toPixels(H.pick(point.x2, point.x + (point.len || 0)), true),
            width = plotX2 - plotX,
            widthDifference;
            
            if (minPointLength) {
                widthDifference = width < minPointLength ? minPointLength - width : 0;
                plotX -= widthDifference / 2;
                plotX2 += widthDifference / 2;
            }
            
            plotX = Math.max(plotX, -10);
            plotX2 = Math.min(Math.max(plotX2, -10), xAxis.len + 10);
            
            point.shapeArgs = {
            x: plotX,
            y: point.plotY + metrics.offset,
            width: plotX2 - plotX,
            height: metrics.width
            };
            point.tooltipPos[0] += width / 2;
            point.tooltipPos[1] -= metrics.width / 2;
        });
    }
    });
    
    /**
     * Max x2 should be considered in xAxis extremes
     */
    H.wrap(H.Axis.prototype, 'getSeriesExtremes', function (proceed) {
        var axis = this,
        dataMax,
        modMax;
        
        proceed.call(this);
        if (this.isXAxis) {
            dataMax = pick(axis.dataMax, Number.MIN_VALUE);
            each(this.series, function (series) {
                each(series.x2Data || [], function (val) {
                    if (val > dataMax) {
                        dataMax = val;
                        modMax = true;
                    }
                });
            });
            if (modMax) {
                axis.dataMax = dataMax;
            }
        }
    });
}(Highcharts));

Highcharts.chart('container_chart', {
    chart: {
    type: 'xrange'
    },
    title: {
    text: 'Summary'
    },
    xAxis: {
    type: 'linear'
    },
    yAxis: {
    title: {
    text: ''
    },
    categories: [catarr[0], catarr[1], catarr[2], catarr[3], catarr[4], catarr[5], catarr[6], catarr[7], catarr[8]],
    reversed: true
    },
    series: [{
    name: 'Q8NCG7',
    pointWidth: 20,
    colorByPoint: true,
    data: dataarr,
    point: {
    events: {
    }
    }
        
    }]
    });


    });

</script>


