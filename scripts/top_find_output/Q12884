logy","STRAND (192-196)",192,196],["Topology","STRAND (212-214)",212,214],["Topology","STRAND (218-227)",218,227],["Topology","STRAND (233-238)",233,238],["Topology","STRAND (241-244)",241,244],["Topology","STRAND (246-251)",246,251],["Topology","STRAND (261-270)",261,270],["Topology","STRAND (291-312)",291,312],["Topology","STRAND (315-323)",315,323],["Topology","STRAND (325-331)",325,331],["Topology","STRAND (337-341)",337,341],["Topology","STRAND (343-345)",343,345],["Topology","STRAND (347-351)",347,351],["Topology","STRAND (364-369)",364,369],["Topology","STRAND (375-382)",375,382],["Topology","STRAND (393-395)",393,395],["Topology","STRAND (397-403)",397,403],["Topology","STRAND (405-413)",405,413],["Topology","STRAND (422-428)",422,428],["Topology","STRAND (430-433)",430,433],["Topology","STRAND (436-440)",436,440],["Topology","STRAND (450-455)",450,455],["Topology","STRAND (460-466)",460,466],["Topology","STRAND (469-471)",469,471],["Topology","STRAND (473-477)",473,477],["Topology","STRAND (479-481)",479,481],["Topology","STRAND (484-489)",484,489],["Topology","STRAND (505-513)",505,513],["Topology","STRAND (516-524)",516,524],["Topology","STRAND (530-532)",530,532],["Topology","STRAND (534-540)",534,540],["Topology","STRAND (568-573)",568,573],["Topology","STRAND (577-580)",577,580],["Topology","STRAND (613-623)",613,623],["Topology","STRAND (637-639)",637,639],["Topology","STRAND (642-648)",642,648],["Topology","STRAND (692-699)",692,699],["Topology","STRAND (703-705)",703,705],["Topology","STRAND (725-729)",725,729]];

        var dataarr = [];
        var catarr = [];
        var i;

        for (i = 0; i < domainElem.length ; i++ )
        {
        switch(domainElem[i][0]) {
            
          case "Sequence":
                catarr[0] = (domainElem[i][0]);
                dataarr.push({
                x:parseInt(domainElem[i][2]),
                x2: parseInt(domainElem[i][3]),
                y:0,
                color: '#e41a1c',
                key: (domainElem[i][1])
                });
                break;

            
            case "Chains":
                catarr[1] = (domainElem[i][0]);
                dataarr.push({
                x:parseInt(domainElem[i][2]),
                x2: parseInt(domainElem[i][3]),
                y:1,
                color: '#377eb8',
                key: (domainElem[i][1])
                });
                break;
            
            case "Topology":
                catarr[2] = (domainElem[i][0]);
                dataarr.push({
                x: parseInt(domainElem[i][2]),
                x2: parseInt(domainElem[i][3]),
                y:2,
